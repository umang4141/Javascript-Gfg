let countEl1 = document.getElementById("count_el");
let countEl = document.getElementById("count_el");
//document.write(countEl);
lapscomppleted = 0;

function increment() {
    for (let i = 0; i < 2; i++) {

        lapscomppleted = lapscomppleted + 1;
    }
    countEl1.innerText = lapscomppleted;
    //alert(countEl1.innerText);

}
increment();
