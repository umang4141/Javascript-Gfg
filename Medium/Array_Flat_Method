function fun1() {
    let numbers = [
        [1, 2],
        [3, 4, [5, [6], 7]]
    ];
    let ar = numbers.flat(0);

    let arr = numbers.flat(1);
    let arr1 = numbers.flat(2);
    let arr2 = numbers.flat(3);
    let arr3 = numbers.flat(4);

    console.log(ar);

    console.log(arr);
    console.log(arr1);
    console.log(arr2);
    console.log(arr3);


}
fun1();
