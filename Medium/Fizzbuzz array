//var count = 1;
var output = [];

function find() {

    for (var count = 1; count <= 100;) {

        if (count % 3 == 0) {
            output.push("fizz");


        } else if (count % 4 == 0) {
            output.push(" buzz ");


        } else if (count % 3 == 0 && count % 4 == 0) {
            output.push(" fizzbuzz ");

        } else {
            output.push(count);


        }
        count++;


    }
    console.log(output);


}
find();
