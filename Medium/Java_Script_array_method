function is_even(arr) {
    return arr % 2 == 0;
}

function fun() {
    var arr = [56, 92, 19, 88, 12];
    var value = arr.every(is_even);
    console.log(value);
}
fun();

function ispositive(element, index, arr, ) {
    return (element > 0);
}

function fun1() {
    var arr = [1, 2, 4, 5, 1, 23, 22];
    var value = arr.every(ispositive);
    console.log(value);
}
fun1()
